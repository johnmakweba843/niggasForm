is:800, \n        trans:'uppercase', \n        padd:10, \n        sty:'italic', \n        paras:\"This is the World of Developers\", \n        bata:'duck', \n        bus:'coster', \n        bos:'boss'\n    }\n  }, \n  methods:{ \n      toggle(){ \n        return this.paras\n      }\n  }\n  \n}\n</script>\n\n<style lang = \"css\">\n  *{ \n    margin:0; \n    padding:0; \n    box-sizing:border-box; \n    list-style:none; \n    text-decoration:none; \n    outline:none; \n    border:none;\n  } \n\n  .active{ \n      width:100%; \n      display:flex; \n      align-items:center; \n      justify-content:space-between; \n      padding:10px 16px; \n      background-color:aquamarine; \n      box-shadow:2px 4px 2px rgba(0 , 0 , 0.5);\n  } \n\n  #navbar{ \n      display:flex; \n      align-items:center; \n      justify-content:center; \n      flex-direction:column;\n  } \n\n  #navbar li{ \n     margin:5px 0;\n  } \n\n  #navbar li a{ \n     color:#000; \n     font-size:18px; \n     font-weight:800; \n     cursor:pointer; \n     text-transform:capitalize; \n     transition:0.3s ease-in-out; \n     font-style:italic;\n  } \n\n  #navbar li a:hover{ \n      color:green;\n  } \n\n  .duck{ \n      display:flex; \n      align-items:center; \n      justify-content:center; \n      flex-direction:column;\n  } \n\n  .coster{ \n      font-size:20px; \n      font-weight:800; \n      font-style:italic; \n      text-transform:uppercase; \n      letter-spacing:1.2px;\n  } \n\n  .boss{ \n      width:100%; \n      max-width:200px; \n      padding:10px 16px; \n      background:rgb(154, 197, 14); \n      color:#000; \n      font-size:18px; \n      font-weight:800; \n      font-style:italic; \n      cursor:pointer; \n      transition:0.3s ease-in-out; \n      box-shadow:2px 4px 2px rgba(200 , 10 , 0.5);\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}